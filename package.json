{
  "name": "@mojaloop/event-sdk",
  "version": "9.4.1",
  "description": "Shared code for Event Logging",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/config",
    "/dist",
    "/protos"
  ],
  "scripts": {
    "ts_app": "tsc ; ts-node src/examples/ts_app.ts",
    "js_app": "tsc ; ts-node src/examples/js_app.js",
    "server": "tsc ; ts-node src/server/server.ts",
    "test": "npm run test:unit | faucet",
    "pretest": "npm run build",
    "standard": "standard",
    "build": "tsc",
    "watch": "tsc -w",
    "test:unit": "jest --testMatch '**/test/unit/**/*.test.ts'",
    "test:junit": "jest --reporters=default --reporters=jest-junit --testMatch '**/test/unit/**/*.test.ts'",
    "test:coverage": "jest --coverage --coverageThreshold='{}' --testMatch '**/test/unit/**/*.test.ts'",
    "test:coverage-check": "jest --coverage --testMatch '**/test/unit/**/*.test.ts'",
    "git:standardFix": "standard --fix",
    "warn-pre-commit": "echo \"pre commit tasks running. build and test\"",
    "audit:resolve": "SHELL=sh resolve-audit",
    "audit:check": "SHELL=sh check-audit",
    "dep:check": "npx ncu -e 2",
    "dep:update": "npx ncu -u"
  },
  "pre-commit": [
    "warn-pre-commit",
    "dep:check",
    "build",
    "git:standardFix",
    "test:unit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojaloop/event-sdk.git"
  },
  "keywords": [
    "event"
  ],
  "author": "ModusBox",
  "contributors": [
    "Georgi Georgiev <georgi.georgiev@modusbox.com>",
    "Miguel de Barros <miguel.debarros@modusbox.com>",
    "Ramiro Gonz√°lez Maciel <ramiro@modusbox.com>",
    "Valentin Genev <valentin.genev@modusbox.com>",
    "Miguel de Barros <miguel.debarros@modusbox.com>",
    "Lewis Daly <lewisd@crosslaketech.com"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mojaloop/event-sdk/issues"
  },
  "homepage": "https://github.com/mojaloop/event-sdk#readme",
  "dependencies": {
    "@grpc/proto-loader": "0.5.3",
    "@mojaloop/central-services-logger": "9.1.0",
    "brototype": "0.0.6",
    "error-callsites": "2.0.3",
    "grpc": "1.24.2",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "parse-strings-in-object": "2.0.0",
    "protobufjs": "6.8.9",
    "rc": "1.2.8",
    "serialize-error": "4.1.0",
    "sinon": "9.0.1",
    "traceparent": "1.0.0",
    "tslib": "1.11.1",
    "uuid4": "1.1.4",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/lodash": "4.14.149",
    "@types/node": "13.9.3",
    "@types/sinon": "7.5.2",
    "@types/tape": "4.2.34",
    "@types/uuid": "7.0.2",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "debug": "4.1.1",
    "faucet": "0.0.1",
    "jest": "25.1.0",
    "jest-junit": "10.0.0",
    "npm-audit-resolver": "2.2.0",
    "npm-check-updates": "4.0.5",
    "nyc": "15.0.0",
    "pre-commit": "1.2.2",
    "proxyquire": "2.1.3",
    "standard": "14.3.3",
    "tap-spec": "5.0.0",
    "tap-xunit": "2.4.1",
    "tape": "4.13.2",
    "tapes": "4.1.0",
    "ts-jest": "25.2.1",
    "ts-node": "8.8.1",
    "tslint": "6.1.0",
    "typedoc": "0.17.3",
    "typescript": "3.3.3333"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "typings"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "standard": {
    "ignore": [
      "/dist",
      "jest.config.js"
    ]
  }
}
